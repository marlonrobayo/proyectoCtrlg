<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>EliminaPersona</servlet-name>
        <servlet-class>Servlet.EliminaPersona</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminaRol</servlet-name>
        <servlet-class>Servlet.EliminaRol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Servlet.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Personas</servlet-name>
        <servlet-class>Servlet.Personas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Roles</servlet-name>
        <servlet-class>Servlet.Roles</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletMarlon</servlet-name>
        <servlet-class>Servlet.ServletMarlon</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EliminaPersona</servlet-name>
        <url-pattern>/EliminaPersona</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminaRol</servlet-name>
        <url-pattern>/EliminaRol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Personas</servlet-name>
        <url-pattern>/Personas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Roles</servlet-name>
        <url-pattern>/Roles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletMarlon</servlet-name>
        <url-pattern>/ServletMarlon</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
